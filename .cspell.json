{
  "version": "0.2",
  "language": "en",
  "import": [
    "@cspell/dict-bash",
    "@cspell/dict-companies",
    "@cspell/dict-en-gb",
    "@cspell/dict-filetypes",
    "@cspell/dict-html",
    "@cspell/dict-node",
    "@cspell/dict-npm",
    "@cspell/dict-software-terms",
    "@cspell/dict-typescript"
  ],
  "allowCompoundWords": true,
  "flagWords": [],
  "ignorePaths": [
    "node_modules/**",
    "pnpm-lock.yaml",
    "package.json",
    "Makefile",
    "**/.github/**",
    "**/.cursor/**",
    "**/.agent/**",
    "**/.husky/**",
    "biome.json",
    "tsconfig.json",
    "tsconfig.build.json",
    ".commitlintrc.js",
    "jest.setup.ts",
    "coverage/**",
    "**/dist/**",
    "build/**",
    "logs/**",
    "logs/**",
    "docs/**",
    "PULL_REQUEST_TEMPLATE.md"
  ],
  "dictionaries": [
    "typescript",
    "softwareTerms",
    "node",
    "en_us",
    "en-gb",
    "npm",
    "html",
    "companies",
    "misc",
    "filetypes",
    "bash",
    "lorem-ipsum",
    "es-es"
  ],
  "words": [
    "Barbieri",
    "Bnumber",
    "bullmq",
    "commitlintrc",
    "Davide",
    "hsts",
    "iends",
    "Ijoi",
    "ilike",
    "istart",
    "jscoverage",
    "knexfile",
    "lintstagedrc",
    "ncurc",
    "nycrc",
    "onal",
    "ossp",
    "pgcrypto",
    "PGWEB",
    "sosedoff",
    "tsyringe",
    "Uncapitalize",
    "Unqueryable",
    "uuidv",
    "versionrc",
    "weakcb"
  ]
}
